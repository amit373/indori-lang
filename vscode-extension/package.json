{
  "name": "indori-lang",
  "displayName": "IndoriLang",
  "description": "IndoriLang v1.0 - Programming language in authentic Indori slang",
  "version": "v1.0",
  "publisher": "indori-lang",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "indori",
        "aliases": [
          "IndoriLang",
          "indori"
        ],
        "extensions": [
          ".il"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "indori",
        "scopeName": "source.indori",
        "path": "./syntaxes/indori.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "indori",
        "path": "./snippets/indori.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:indori"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/indori-lang/indori-lang.git"
  },
  "keywords": [
    "indori",
    "programming language",
    "indori lang",
    "compiler"
  ],
  "author": "IndoriLang Team",
  "license": "MIT",
  "icon": "icon.png"
}
