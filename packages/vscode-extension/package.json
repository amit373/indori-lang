{
  "name": "indori-lang",
  "displayName": "IndoriLang",
  "description": "IndoriLang v1.0 - Programming language in authentic Indori slang",
  "version": "1.0.0",
  "publisher": "indori-lang",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "indori",
        "aliases": [
          "IndoriLang",
          "indori"
        ],
        "extensions": [
          ".il"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "indori",
        "scopeName": "source.indori",
        "path": "./syntaxes/indori.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "indori",
        "path": "./snippets/indori.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "echo 'No build required - declarative extension'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amit373/indori-lang.git"
  },
  "keywords": [
    "indori",
    "programming language",
    "indori lang",
    "compiler"
  ],
  "author": "IndoriLang Team",
  "license": "MIT",
  "icon": "icon.png"
}
